{
    "name": "Translation+",
    "version": "1.0",
    "description": "Translation+",
    "permissions": [
        "http://dict.cn/", "https://www.googleapis.com/language/translate/v2"
    ],
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "icon.png",
        "popup": "popup.html"
    },
    "background_page": "background.html",
    "content_scripts": [
        {
        "matches": ["*://*/*"],
        "js": ["translation_plus.js"]
        }
    ]
} 
